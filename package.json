{
  "name": "bs-cli",
  "description": "Common build scripts for my rbxts projects.",
  "version": "1.0.0",
  "author": "Daymon Littrell-Reyes",
  "bin": {
    "bs": "./bin/run.js",
    "bs-dev": "./bin/dev.js"
  },
  "bugs": "https://github.com/daymxn/bs-cli/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@types/ejs": "^3.1.5",
    "@verdaccio/config": "6.0.0-6-next.76",
    "ejs": "^3.1.10",
    "eslint": "^8.57.1",
    "execa": "^9.5.1",
    "fs-extra": "^11.2.0",
    "github-slugger": "^2.0.0",
    "jsonc-parser": "^3.3.1",
    "lodash-es": "^4.17.21",
    "normalize-package-data": "^7.0.0",
    "pm2": "^5.4.3",
    "strip-ansi": "^7.1.0",
    "tmp-promise": "^3.0.3",
    "verdaccio": "^6.0.2",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.5"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.9.0",
    "@types/normalize-package-data": "^2.4.4",
    "@verdaccio/types": "^10.8.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "eslint-plugin-headers": "^1.2.0",
    "eslint-plugin-prettier": "^5.2.1",
    "oclif": "^4",
    "prettier-plugin-organize-imports": "^4.1.0",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@changesets/cli": "^2",
    "@microsoft/api-documenter": "^7",
    "@microsoft/api-extractor": "^7",
    "eslint": ">=8.0.0",
    "tsup": "^8",
    "ts-node": "^10"
  },
  "peerDependenciesMeta": {
    "@microsoft/api-extractor": {
      "optional": true
    },
    "@microsoft/api-documenter": {
      "optional": true
    },
    "tsup": {
      "optional": true
    },
    "eslint": {
      "optional": true
    },
    "@changesets/cli": {
      "optional": true
    },
    "ts-node": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=22.9.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json",
    "/static/theme.json",
    "/static/assets"
  ],
  "homepage": "https://github.com/daymxn/bs-cli",
  "keywords": [
    "oclif",
    "bs",
    "cli",
    "rbxts"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "bs",
    "dirname": "bs",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "helpClass": "./dist/help",
    "theme": "./static/theme.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daymxn/bs-cli.git"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && pnpm assets",
    "assets": "cp -rf ./static/assets ./dist/assets",
    "dev": "pnpm exec ./bin/dev.cmd",
    "prod": "pnpm exec ./bin/run.cmd",
    "reprod": "pnpm exec ./bin/run.cmd",
    "prereprod": "pnpm build",
    "format": "eslint . --fix",
    "manifest": "oclif manifest",
    "readme": "pnpm reprod readme",
    "update": "pnpm readme && pnpm manifest",
    "preupdate": "shx rm -f oclif.manifest.json"
  },
  "imports": {
    "#src/*": "./dist/*"
  },
  "types": "dist/index.d.ts"
}
