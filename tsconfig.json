{
  "compilerOptions": {
    "declaration": false,
    "module": "Node16",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "target": "es2022",
    "moduleResolution": "node16",
    "baseUrl": ".",
		"paths": {
			"#src/*": ["src/*"],
    },
    "skipLibCheck": true
  },
  "include": ["./src/**/*"],
  "ts-node": {
    "esm": true
  },
  "exclude": [
    "node_modules",
  ]
}
